{
    "name": "firephp-for-firebug.next-components",
    "uid": "github.com/firephp/firephp-for-firebug.next/components",
    "main": "./main.js",
    "exports": {
        "bundles": {
            "main.js": "./main.js"
        }
    },
    "layout": {
        "directories": {
            "bundles": "./.pinf.program"
        }
    },
    "imports": {
        "globals": {
            "Components": true,
            "console": true,
            "insight": true
        }
    },
    "upstream": {
        "packages": {
            "top": [
                "/genesis/os.inception/services/*/*",
                "/opt/services/*"
            ]
        }
    },
    "mappings": {
        "wildfire-for-js": "top/wildfire-for-js"
    },
    "dependencies": {
        "eventemitter2": "~0.4.14",
        "firebug.sdk": "0.5.x"
    },
    "scripts": {
        "install": "../node_modules/.bin/smi install -vd"
    },
    "config": {
        "smi.cli": {
            "packagesDirectory": "node_modules",
            "latestOnly": true
        }
    }
}